{
    "version": 2,
    "routes": [
        {
            "src": "(.*)",
            "dest": "src/index.ts",
            "methods": [
                "GET",
                "POST",
                "PUT"
            ]
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Referrer-Policy",
                    "value": "origin-when-cross-origin"
                }
            ]
        }
    ]
}
